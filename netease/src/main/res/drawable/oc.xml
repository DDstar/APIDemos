<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="1.0dip" />
            <stroke android:width="3.0dip" android:color="#ffff0000" />
            <size android:height="30.0dip" android:width="30.0dip" />
            <gradient android:startColor="#00000000" android:endColor="#00000000" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="1.0dip" />
            <stroke android:width="2.0dip" android:color="#fffe5553" />
            <size android:height="20.0dip" android:width="20.0dip" />
            <gradient android:startColor="#00000000" android:endColor="#00000000" />
        </shape>
    </item>
</selector>