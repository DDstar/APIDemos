<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="#ffd8f1ff" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="#fff9fdff" />
            <stroke android:width="@dimen/x_" android:color="#9da3d1fb" android:dashWidth="@dimen/x9" android:dashGap="@dimen/x8" />
        </shape>
    </item>
</selector>